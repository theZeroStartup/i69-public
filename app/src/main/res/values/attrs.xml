<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckableImageButton">
        <attr name="checkedSrc" format="reference" />
        <attr name="uncheckedSrc" format="reference" />
        <attr name="src" format="reference" />
        <attr name="text" format="string" />
        <attr name="colorDefault" format="color" />
        <attr name="colorChecked" format="color" />
        <attr name="tintColorDefault" format="color" />
        <attr name="tintColorChecked" format="color" />
        <attr name="orientation" format="enum">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="InsLoadingViewAttr">
<!--        <attr name="rotate_duration" format="integer" />-->
        <attr name="circle_duration" format="integer" />
        <attr name="start_color" format="color" />
        <attr name="end_color" format="color" />
        <attr name="status" >
            <enum name="loading" value="0" />
            <enum name="clicked" value="1" />
            <enum name="unclicked" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ToggleImageView">
        <attr name="checkedDrawable" format="integer" />
        <attr name="uncheckedDrawable" format="integer" />
    </declare-styleable>

    <declare-styleable name="ChipCloud">
        <attr name="deselectedColor" format="color" />
        <attr name="deselectedFontColor" format="color" />
        <attr name="selectedColor" format="color" />
        <attr name="selectedFontColor" format="color" />
        <attr name="selectTransitionMS" format="integer" />
        <attr name="deselectTransitionMS" format="integer" />
        <attr name="chipMaxSelectedCounter" format="integer" />
        <attr name="selectMode">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
            <enum name="required" value="2" />
            <enum name="none" value="3" />
        </attr>
        <attr name="gravity">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="center" value="2" />
            <enum name="staggered" value="3" />
        </attr>
        <attr name="typeface" format="string" />
        <attr name="allCaps" format="boolean" />
        <attr name="textSize" format="dimension" />
        <attr name="labels" format="reference" />
        <attr name="minHorizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
    </declare-styleable>
</resources>